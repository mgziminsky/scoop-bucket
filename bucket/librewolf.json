{
    "version": "96.0.3",
    "description": "A fork of Firefox, focused on privacy, security and freedom.",
    "homepage": "https://librewolf-community.gitlab.io/",
    "license": "MPL-2.0",
    "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/5619a0f68ce8ebafd66bbbe2c655505c/librewolf-96.0.3.en-US.win64.zip",
    "hash": "d2ec7b9ee450c4e1547595cd297c306f87cfb43d3ac448cce0880d311efd1db4",
    "extract_dir": "librewolf-96.0.3/LibreWolf",
    "bin": [
        [
            "librewolf.exe",
            "LibreWolf",
            "-profile \"$dir\\profiles\\default\""
        ]
    ],
    "shortcuts": [
        [
            "librewolf.exe",
            "LibreWolf",
            "-profile \"$dir\\profiles\\default\""
        ]
    ],
    "persist": "profiles",
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "jsonpath": "$[0].assets.links",
        "regex": "(?s)(?=.*(?<url>https://.+?/librewolf-(?<version>[\\d\\.]+)\\.en-US\\.win64\\.zip))(?=.*(?<hashUrl>https://.+?/sha256sums\\.txt))"
    },
    "autoupdate": {
        "url": "$matchUrl",
        "extract_dir": "librewolf-$matchHead/LibreWolf",
        "hash": {
            "url": "$matchHashurl"
        }
    }
}
